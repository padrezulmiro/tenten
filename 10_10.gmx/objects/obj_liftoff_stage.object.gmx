            <string>///Draw liftoff drawable objs

// Draw tracing line
var DENOM = 41;
for (var i = 0; i &lt; DENOM; i += 2) {
    var init_h = i*room_height/DENOM;
    var final_h = (i+1)*room_height/DENOM;
    
    if final_h &lt; room_height {   
        draw_line_width_colour(START_XX, init_h, START_XX, final_h, 2, c_white, c_white);
    } else {
        draw_line_width_colour(START_XX, init_h, START_XX, room_height, 2, c_white, c_white);
    }
} 

// Draw all drawable objs
if ds_exists(drawable_objs, ds_type_list) {
    for (var i = 0; i &lt; ds_list_size(drawable_objs); i++) {
        var d = drawable_objs[| i];
        var s = object_get_sprite(d[? 'obj_ind']);
        
        if sprite_exists(s) {    
            draw_sprite
            (
                s,
                0,
                d[? 'xx'],
                d[? 'yy']
            );
        }
    }       
}

// Draw the liftoff planet
var spr = asset_get_index('spr_ship');
draw_sprite(spr, 0, START_XX, mouse_y);

